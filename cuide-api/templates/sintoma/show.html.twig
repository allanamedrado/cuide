{% extends 'base.html.twig' %}

{% block title %}Sintoma{% endblock %}

{% block body %}
    <h1>Sintoma</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>SintomaId</th>
                <td>{{ sintoma.sintomaId }}</td>
            </tr>
            <tr>
                <th>Alteracoes</th>
                <td>{{ sintoma.alteracoes ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>LocalAlteracoes</th>
                <td>{{ sintoma.localAlteracoes }}</td>
            </tr>
            <tr>
                <th>LocalDores</th>
                <td>{{ sintoma.localDores }}</td>
            </tr>
            <tr>
                <th>Observacoes</th>
                <td>{{ sintoma.observacoes }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sintoma_index') }}">back to list</a>

    <a href="{{ path('sintoma_edit', {'sintomaId': sintoma.sintomaId}) }}">edit</a>

    {{ include('sintoma/_delete_form.html.twig') }}
